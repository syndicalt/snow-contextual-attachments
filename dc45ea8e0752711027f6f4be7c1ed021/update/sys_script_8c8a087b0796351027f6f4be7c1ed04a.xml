	var filterCondition = current.getValue("filter_condition");
	var gr = new GlideRecord("x_975207_context_0_upload_contexts");
	gr.setLimit(1);
	gr.addQuery("sys_id","!=", sysId);
	gr.addQuery("active", true);
	gr.addQuery("table", table);
	gr.addQuery("filter_condition", filterCondition);
	gr.query();

